<?xml version="1.0" encoding="utf-8"?>
<launch>
  <node pkg="starmap" type="starmap_node" name="starmap" launch-prefix="gdb -ex run -args" output="screen">
    <param name="image_topic" value="image_raw_0" />
    <param name="bbox_topic" value="sort_ros/tracked_bounding_boxes_0" />
    <param name="keypoint_topic" value="keypoints_0" />
    <param name="starmap_model_path" value="$(find starmap)/models/model_cpu-jit.pth" />
    <param name="gpu_id" value="-1" type="int" />
  </node>
</launch>
